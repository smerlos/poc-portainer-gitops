branches:
  - name: main

    prerelease: true
tagFormat: ${version}
plugins:
  - - '@semantic-release/commit-analyzer'
    - releaseRules:
        - release: patch
  - - '@semantic-release/git'
    - assets:
        - app/docker-compose.yml
      message: 'chore(release): ${nextRelease.version} [skip ci]'
verifyConditions:
  - '@semantic-release/git'
